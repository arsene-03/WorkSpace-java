package ex01;

public class Main {

	public static void main(String[] args) {
		Value01 v1 = new Value01(5);
		Value01 v2 = new Value01(5);
		
		if(v1.equals(v2)) {
			System.out.println("v1과 v2는 같습니다");
		}else {
			System.out.println("v1과 v2는 다릅니다.");
		}
		
		System.out.println("=================================");
		
		v2 = v1;
		
		if(v1.equals(v2)) {
			System.out.println("v1과 v2는 같습니다");
		}else {
			System.out.println("v1과 v2는 다릅니다.");
		}
	}

}
